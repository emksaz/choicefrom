doctype html
html
    head
        include layout/head.jade
        +title ('发布')
    body
        include layout/loading
        .main-wrapper
            include layout/nav
            header.page-header
                .w-container
                    a
                        i.ic.ic-24-dashboard-l
                    h1 Here are some quick tips for a first-time organization member.
                    button.purple
                        i.ic.ic-brush-w
                        span 设计问卷
                    include ./template/shortcut
            .w-container
                .tabs.tabs-style
                    include ./template/tabs-settings
                    #published.content-wrap
                        .content-row
                            .row-title
                                h2 发布
                                #publishtext 当前收集器状态：在线
                                #RestTime 剩余时间：8小时16分
                                button.blue
                                    i.ic.ic-plus-w
                                    span 添加收集方案
                                button.green(disabled='disabled')
                                    i.ic.ic-2dot-w
                                    span 保存更改
                        .tabs.tabs-style-sub
                            nav
                                ul
                                    li.sub-tab-current
                                        i.dot.blue
                                        span 收集方案 1
                                    li
                                        i.dot.purple
                                        span 收集方案 2
                                    li
                                        i.dot.gray
                                        span 收集方案 3
                                .dropdown-event
                                    include ./template/floating-strip-3
                                    +item('选中收集方案操作：','复制','重命名','删除')
                            include ./template/publish-web-link
                            include ./template/publish-setting
                            include ./template/publish-collection
    include layout/footer