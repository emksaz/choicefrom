                    <h3 class="float_l">&#35843;&#35797;</h3>
                </div>
                <div class="w-clearfix w-inline-block design_control_bar_button float_r
                     padding_tb_8px font_center padding_lr_20px border_dark_gary_262626 left cursor_pointer"
                     ng-click="handleSaveClick()" title="&#20445;&#23384;">
                    <div class="design_icon size_20px float_l ic_uploading data_no_change"
                         ng-class="{'data_saving':designManager.isAutoSaving(),'data_no_change':!designManager.hasChange()}" title="&#20445;&#23384;"></div>
                    <h3 class="float_l" style="color: #887D7D;margin-left:5px;">&#20445;&#23384;</h3>
                </div>
                <div class="w-inline-block float_r cursor_pointer"
                     
                     ng-mouseenter="handleVersionHistoryMouseEnter()"
                     ng-mouseleave="handleVersionHistoryMouseLeave()">
                    <div data-version-history="designManager.versionHistoryManager"
                         ng-if="designManager.versionHistoryManager.isVisible()"></div>
                    <div class="w-clearfix w-inline-block design_control_bar_button float_r padding_tb_8px
                         font_center padding_lr_20px border_dark_gary_262626 left">
                        <div class="design_icon size_20px float_l ic_history"
                             ng-class="{'data_saving':designManager.isManaulSaving()}" title="&#20445;&#23384;&#29256;&#26412;"></div>
                    </div>
                </div>
                <div class="w-clearfix float_r padding_lr_10px border_dark_gary_262626 left cursor_pointer" style="display:none;">
                    <div class="w-clearfix w-inline-block design_control_bar_button float_r padding_tb_8px font_center padding_lr_5px">
                        <div class="design_icon size_20px float_l ic_redo"></div>
                    </div>
                    <div class="w-clearfix w-inline-block design_control_bar_button float_r padding_tb_8px font_center padding_lr_5px">
                        <div class="design_icon size_20px float_l ic_undo"></div>
                    </div>
                </div>
                <div class="w-inline-block float_r cursor_pointer"
                     ng-mouseenter="handleSearcherMouseEnter($event)"
                     ng-mouseleave="handleSearcherMouseLeave($event)"
                     id="searcher">
                    <div class="w-inline-block search_button color_dark_slate_gary_kit float_r"
                         ng-mouseenter="searchBtnHover = true" ng-mouseleave="searchBtnHover = false">
                        <div class="design_icon size_20px ic_search" ng-click="handleSearchClick()"
                             ng-class="{'opacity_100_dur_200':searchBtnHover,'opacity_75_dur_200':!searchBtnHover}" title="&#25628;&#32034;"></div>
                    </div>
                    <div class="search_symbol border_dark_gary_262626 left color_dark_slate_gary_kit dur_width_200">
                        <div class="w-form search_form_wrapper">
                            <input class="w-input scarch color_dark_gary_gradient_input design_input border_dark_gary_262626 all_sides font_color_light_d6d6d6"
                                   id="search_txt" type="text" placeholder="&#25628;&#32034;" ng-keydown="handleSearcherKeydown($event)">
                        </div>
                    </div>
                </div>

            </header>
            <!--&#19978;&#20391;&#25805;&#20316;&#26639;&#9650;-->
            <!--&#28155;&#21152;&#33410;&#28857;&#25805;&#20316;&#26639;&#9660;-->
            <div data-node-box="designManager.nodeBoxManager" data-context-help="designManager.helpManager" ng-if="designManager.nodeBoxManager.isVisible()"></div>
            <!--&#28155;&#21152;&#33410;&#28857;&#25805;&#20316;&#26639;&#9650;-->
            <!--&#20027;&#39064;&#25805;&#20316;&#26639;&#9660;-->
            <div data-theme-box="designManager.themeBoxManager" ng-if="designManager.themeBoxManager.isVisible()"></div>
            <!--&#20027;&#39064;&#25805;&#20316;&#26639;&#9650;-->
        </main>
    </section>
    <div data-node-searcher="designManager" ng-if="designManager.searchManager.show"></div>
    <div data-node-validator="designManager" ng-if="designManager.validateManager.show"></div>
    <div data-batch-option="designManager.optionBatchAddManager" ng-if="designManager.optionBatchAddManager.isVisible()"></div>
    <div data-version-save="designManager.versionSaveManager" ng-if="designManager.versionSaveManager.isVisible()"></div>
    <div data-context-menu="designManager.contextMenu" ng-if="designManager.contextMenu.isVisible()"></div>
    <div data-help-box="designManager" data-help-text="designManager.helpManager" ng-if="designManager.helpManager.show"></div>
