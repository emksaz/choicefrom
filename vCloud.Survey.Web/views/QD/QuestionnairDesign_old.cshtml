                        <div class="design_icon size_20px float_l ic_zoomin"></div>
                    </div>
                    <h3 class="float_r padding_tb_5px padding_lr_5px width_58px text_align_center cursor_default" ng-bind="designManager.getChartZoomRateText()"></h3>
                    <div class="w-clearfix w-inline-block design_buttom_bar_button float_r padding_tb_5px padding_lr_5px cursor_pointer"
                         title="&#32553;&#23567;" ng-click="handleZoomOutClick()">
                        <div class="design_icon size_20px float_l ic_zoomout"></div>
                    </div>
                </div>
                <div class="w-clearfix float_r padding_lr_5px border_light_gary_3c3c3c left cursor_pointer" style="display:none;">
                    <div class="w-clearfix w-inline-block design_buttom_bar_button float_r padding_tb_5px padding_lr_5px"
