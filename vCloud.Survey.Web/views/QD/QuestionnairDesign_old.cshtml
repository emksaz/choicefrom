                        <div class="design_icon size_20px float_l ic_duplicate"></div>
                    </div>
                    <div class="w-clearfix w-inline-block design_buttom_bar_button float_r padding_tb_5px padding_lr_5px cursor_pointer"
                         title="&#26029;&#24320;&#25152;&#36873;&#36830;&#32447;" ng-click="handleBreakConnClick()">
                        <div class="design_icon size_20px float_l ic_broken_link"></div>
                    </div>
                </div>
                <span id="system_hinter" class="font_size_12px"></span>
            </div>
            <!--&#19979;&#20391;&#25805;&#20316;&#26639;&#9650;-->
            <!--&#19978;&#20391;&#25805;&#20316;&#26639;&#9660;-->
            <header class="w-clearfix design_control color_dark_slate_gary_kit border_dark_gary_262626 buttom click_hide_unpinned">
                <div class="padding_lr_8px float_l cursor_pointer">
                    <div class="w-inline-block toolbar_button"
                         ng-click="handleBackIconClick()"
                         ng-mouseenter="backHover = true;"
                         ng-mouseleave="backHover = false;">
                        <div class="design_icon size_20px ic_back_to_dashboard"></div>
                    </div>
                </div>
                <div class="w-clearfix design_que_title float_l padding_lr_10px font_color_light_d6d6d6 cursor_default"
                     ng-if="!backHover">
                    @*<div class="design_icon size_24px ic_logo float_l margin_r_10px"></div>*@
                    <div class="overflow_hide font_line_height_24px height_24px text_ellipsis" id="start_node_name" title="{{designManager.startNode.nodeName}}" ng-bind="designManager.startNode.nodeName"></div>
                </div>
                <div class="back_to_dashboard float_l font_color_light_d6d6d6 padding_lr_10px cursor_pointer elementShow"
