                <div class="w-inline-block toolbar_button border_dark_gary_262626 buttom cursor_pointer">
                    <div class="design_icon size_20px ic_nodes_kit"
                         ng-click="handleToggleNodeBoxClick();" title="&#33410;&#28857;&#21015;&#34920;"></div>
                </div>
                <div class="w-inline-block toolbar_button border_dark_gary_262626 buttom cursor_pointer">
                    <div class="design_icon size_20px ic_theme"
                         ng-click="handleToggleThemeBoxClick()"></div>
                </div>
                <div class="absolute_b padding_lr_8px" style="display:none">
                    <div class="w-inline-block toolbar_button border_dark_gary_262626 buttom cursor_pointer" style="display:none">
                        <div class="design_icon size_20px ic_cooperative"></div>
                    </div>
                    <div class="w-inline-block toolbar_button cursor_pointer" style="display:none">
                        <div class="design_icon size_20px ic_prompt"></div>
                    </div>
                    <div class="w-inline-block toolbar_button">
                        <div class="design_icon size_20px ic_video_tutorials cursor_pointer"></div>
                    </div>
                    <div class="w-inline-block toolbar_button cursor_pointer">
                        <div class="design_icon size_20px ic_help"></div>
                    </div>
                </div>
            </div>
            <!--&#24038;&#20391;&#36873;&#39033;&#26639;&#9650;-->
            <!--&#19979;&#20391;&#25805;&#20316;&#26639;&#9660;-->
            <div class="w-clearfix design_buttom_bar color_light_buttom_bar border_light_gary_3c3c3c top click_hide_unpinned">
                <div class="w-clearfix float_r padding_lr_5px border_light_gary_3c3c3c left">
                    <div class="w-clearfix w-inline-block design_buttom_bar_button float_r padding_tb_5px padding_lr_5px cursor_pointer"
